[
    {
        "rule00I": {
            "Context": "/md:EntityDescriptor[1]",
            "Message": "Validating entityID https://idp.example.org/idp.xml",
            "Rule": "@entityID",
            "Severity": "Info"
        }
    },
    {
        "rule03W": {
            "Context": "/md:EntityDescriptor[1]/md:IDPSSODescriptor[1]",
            "Message": "Each IDPSSODescriptor should contain NameIDFormat with one or more values",
            "Rule": "md:NameIDFormat[text() != '']",
            "Severity": "Warning"
        }
    },
    {
        "rule05W": {
            "Context": "/md:EntityDescriptor[1]",
            "Message": "EntityDescriptor should contain an Organization",
            "Rule": "md:Organization",
            "Severity": "Warning"
        }
    },
    {
        "rule06W": {
            "Context": "/md:EntityDescriptor[1]",
            "Message": "EntityDescriptor should contain ContactPerson with a contactType of \"support\" and at least one EmailAddress",
            "Rule": "md:ContactPerson[normalize-space(@contactType)='support'][md:EmailAddress]",
            "Severity": "Warning"
        }
    },
    {
        "rule07W": {
            "Context": "/md:EntityDescriptor[1]",
            "Message": "EntityDescriptor should contain ContactPerson with a contactType of \"technical\" and at least one EmailAddress",
            "Rule": "md:ContactPerson[normalize-space(@contactType)='technical'][md:EmailAddress]",
            "Severity": "Warning"
        }
    },
    {
        "rule12E": {
            "Context": "/md:EntityDescriptor[1]/md:IDPSSODescriptor[1]",
            "Message": "IDPSSODescriptor/SPSSODescriptor must contain alg:DigestMethod",
            "Rule": "descendant::alg:DigestMethod",
            "Severity": "Error"
        }
    },
    {
        "rule13E": {
            "Context": "/md:EntityDescriptor[1]/md:IDPSSODescriptor[1]",
            "Message": "IDPSSODescriptor/SPSSODescriptor must contain SigningMethod",
            "Rule": "descendant::alg:SigningMethod",
            "Severity": "Error"
        }
    },
    {
        "rule15W": {
            "Context": "/md:EntityDescriptor[1]/md:IDPSSODescriptor[1]",
            "Message": "IDPSSODescriptor/SPSSODescriptor should contain SingleLogoutService",
            "Rule": "md:SingleLogoutService",
            "Severity": "Warning"
        }
    },
    {
        "rule32E": {
            "Context": "/md:EntityDescriptor[1]/md:IDPSSODescriptor[1]",
            "Message": "IDPSSODescriptor  must contain a mdui:UIInfo element",
            "Rule": "md:Extensions/mdui:UIInfo",
            "Severity": "Error"
        }
    },
    {
        "rule60E": {
            "Context": "/md:EntityDescriptor[1]/md:IDPSSODescriptor[1]",
            "Message": "Each IDPSSODescriptor/SPSSODescriptor must contain a signing key as X509Certificate (child element of X509Data)",
            "Rule": "md:KeyDescriptor[@use='signing' or not(@use)]/ds:KeyInfo/ds:X509Data/ds:X509Certificate",
            "Severity": "Error"
        }
    },
    {
        "Summary": {
            "ERROR": 4,
            "INFO": 1,
            "OK": 18,
            "WARNING": 5
        }
    }
]